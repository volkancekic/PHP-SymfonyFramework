{% extends 'frontbase.html.twig' %}


{% block title %} Profil Bilgileri {% endblock %}

{% block content %}



<section class="main-content">
    <div class="row">
        <div class="span9">
            <h4>Profil Bilgileri</h4>
            {{ include('messages.html.twig') }}

            <table class="table">
                <thead>

            <tr>
                <th>Kullanıcı id</th>
                <td>{{ app.user.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ app.user.email }}</td>
            </tr>
            <tr>
                <th>Yetki</th>
                <td>{{ app.user.roles|join }}</td>
            </tr>
            <tr>
                <th>Ad Soyad</th>
                <td>{{ app.user.name }}</td>
            </tr>
            <tr>
                <th>Adres</th>
                <td>{{ app.user.address }}</td>
            </tr>
            <tr>
                <th>Şehir</th>
                <td>{{ app.user.city }}</td>
            </tr>
            <tr>
                <th>Telefon</th>
                <td>{{ app.user.phone }}</td>
            </tr>
            <tr>
                <th>Durum</th>
                <td>{{ app.user.status }}</td>
            </tr>

        </tbody>
    </table>






        </div>
    </div>






    <script src="{{ asset('assets') }}/js/common.js"></script>

</section>

{% endblock %}
